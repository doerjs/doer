    "**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/runtime": "^7.16.7",
    "@babel/runtime-corejs3": "^7.16.7",
    "<%= packages.cli.name %>": "<%= packages.cli.version %>",
    "<%= packages.eslint.name %>": "<%= packages.eslint.version %>",
    "<%= packages.prettier.name %>": "<%= packages.prettier.version %>",
    <% if (answers.style === 'less') { %>
    "<%= packages.pluginLess.name %>": "<%= packages.pluginLess.version %>",
    <% } %>
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5"
  }
